@model SportEventsApp.Models.RegularInfo
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>@Model.Title</h2>
<div class="points">
    <ul class="list-group">
        @foreach (var item in Model.Points)
        {
            <li class="list-group-item">@item.Value</li>
        }
    </ul>
</div>